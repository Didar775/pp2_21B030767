import psycopg2
from config import config
# config=psycopg2.connect(
#     host='localhost'
#     user='postgres'
#     password='Zangar.2012'
#     database='postgres'
#     port='5432'
# ) 
def connect():
    conn = None
    try:
        params = config()
        conn = psycopg2.connect(**params)
        cur = conn.cursor()
        cur.execute('select version()')
        db_version = cur.fetchone()
        print(db_version)

    except Exception as e:
        print(str(e))
    if conn is not None:
        conn.close()
connect() 